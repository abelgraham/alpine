#: pith/helptext.c:20281 pith/helptext.c:20829
msgid "For example, &quot;Oct&quot;."
msgstr ""

#: pith/helptext.c:20285
msgid "<DT>MONTHLONG</DT>"
msgstr ""

#: pith/helptext.c:20287 pith/helptext.c:20295 pith/helptext.c:20303
msgid ""
"This token represents the month in which the message was sent, according"
msgstr ""

#: pith/helptext.c:20289 pith/helptext.c:20822
msgid "For example, &quot;October&quot;."
msgstr ""

#: pith/helptext.c:20293
msgid "<DT>MONTH</DT>"
msgstr ""

#: pith/helptext.c:20297 pith/helptext.c:20807
msgid "For example, &quot;10&quot; or &quot;9&quot;."
msgstr ""

#: pith/helptext.c:20301
msgid "<DT>MONTH2DIGIT</DT>"
msgstr ""

#: pith/helptext.c:20305 pith/helptext.c:20814
msgid "For example, &quot;10&quot; or &quot;09&quot;."
msgstr ""

#: pith/helptext.c:20310
msgid "<DT>YEAR</DT>"
msgstr ""

#: pith/helptext.c:20312 pith/helptext.c:20320
msgid "This token represents the year the message was sent, according"
msgstr ""

#: pith/helptext.c:20314 pith/helptext.c:20836
msgid "For example, &quot;1998&quot; or &quot;2001&quot;."
msgstr ""

#: pith/helptext.c:20318
msgid "<DT>YEAR2DIGIT</DT>"
msgstr ""

#: pith/helptext.c:20322 pith/helptext.c:20843
msgid "For example, &quot;98&quot; or &quot;01&quot;."
msgstr ""

#: pith/helptext.c:20327
msgid "<DT>TIME24</DT>"
msgstr ""

#: pith/helptext.c:20329 pith/helptext.c:20340
msgid "This token represents the time at which the message was sent, according"
