#: pith/helptext.c:2031
msgid ""
"  <li><a href=\"h_config_change_your_from\">Changing your From Address</a></"
"li>"
msgstr ""

#: pith/helptext.c:2032
msgid "  <li><a href=\"h_compose_send\">Send Command</a></li>"
