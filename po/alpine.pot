msgstr ""

#: pith/helptext.c:24698 pith/helptext.c:24783
msgid " <LI> <A HREF=\"h_index_sort_date\">Date</A>"
msgstr ""

#: pith/helptext.c:24699 pith/helptext.c:24784
msgid " <LI> <A HREF=\"h_index_sort_subj\">Subject</A>"
