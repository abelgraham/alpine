#: pith/helptext.c:25105
msgid "<TR> <TD>GBK</TD> <TD>Latin and Chinese Simplified</TD> </TR>"
msgstr ""

#: pith/helptext.c:25106
msgid "<TR> <TD>GB2312</TD> <TD>Latin and Chinese Simplified</TD> </TR>"
msgstr ""

#: pith/helptext.c:25107
msgid "<TR> <TD>CN-GB</TD> <TD>Latin and Chinese Simplified</TD> </TR>"
msgstr ""

#: pith/helptext.c:25108
msgid "<TR> <TD>BIG5</TD> <TD>Latin and Chinese Traditional</TD> </TR>"
