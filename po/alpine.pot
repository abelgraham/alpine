msgid ""
"(<!--chtml if pinemode=\"function_key\"-->F11<!--chtml else-->R<!--chtml "
"endif-->)"
msgstr ""

#: pith/helptext.c:6820
msgid "and Forwarding "
msgstr ""

#: pith/helptext.c:6821
msgid ""
"(<!--chtml if pinemode=\"function_key\"-->F12<!--chtml else-->F<!--chtml "
