msgstr ""

#. TRANSLATORS: Start of new help topic.
#: pith/helptext.c:32366
msgid "<TITLE>FEATURE: Enable-Msg-View-URLs</TITLE>"
msgstr ""

#: pith/helptext.c:32369
msgid "<H1>FEATURE: Enable-Msg-View-URLs</H1>"
msgstr ""

#: pith/helptext.c:32371
msgid "Setting this feature causes Alpine to select possible URLs from the"
