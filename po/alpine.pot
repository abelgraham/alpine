#: pith/helptext.c:6055
msgid "<TITLE>Keyboard Lock Command</TITLE>"
msgstr ""

#: pith/helptext.c:6058
msgid "<H1>Keyboard Lock Command</H1>"
msgstr ""

#: pith/helptext.c:6061
