#: pith/helptext.c:7007
msgid "<A HREF=\"h_config_sigdashes\">&quot;Enable-Sigdashes&quot;</A>, and"
msgstr ""

#: pith/helptext.c:7008
msgid ""
"<A HREF=\"h_config_strip_sigdashes\">&quot;Strip-From-Sigdashes-On-"
