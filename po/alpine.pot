msgstr ""

#: pith/helptext.c:25093
msgid ""
"<TR> <TD>ISO-8859-10</TD> <TD>8 bit European \"Latin 6\" character set</TD> "
"</TR>"
msgstr ""

#: pith/helptext.c:25094
msgid "<TR> <TD>ISO-8859-11</TD> <TD>Latin and Thai</TD> </TR>"
msgstr ""

#: pith/helptext.c:25095
msgid "<TR> <TD>ISO-8859-12</TD> <TD>Reserved</TD> </TR>"
msgstr ""

#: pith/helptext.c:25096
msgid ""
"<TR> <TD>ISO-8859-13</TD> <TD>8 bit European \"Latin 7\" character set</TD> "
"</TR>"
msgstr ""

#: pith/helptext.c:25097
msgid ""
"<TR> <TD>ISO-8859-14</TD> <TD>8 bit European \"Latin 8\" character set</TD> "
"</TR>"
msgstr ""

#: pith/helptext.c:25098
msgid ""
"<TR> <TD>ISO-8859-15</TD> <TD>8 bit European \"Latin 9\" character set</TD> "
"</TR>"
msgstr ""

#: pith/helptext.c:25099
msgid ""
"<TR> <TD>ISO-8859-16</TD> <TD>8 bit European \"Latin 10\" character set</TD> "
"</TR>"
msgstr ""

#: pith/helptext.c:25100
msgid "<TR> <TD>KOI8-R</TD> <TD>8 bit Latin and Russian</TD> </TR>"
msgstr ""

#: pith/helptext.c:25101
msgid "<TR> <TD>KOI8-U</TD> <TD>8 bit Latin and Ukranian</TD> </TR>"
