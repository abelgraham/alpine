#: pith/helptext.c:6845
msgid ""
"<A HREF=\"h_config_include_header\">&quot;Include-Header-In-Reply&quot;</A>,"
msgstr ""

#: pith/helptext.c:6846
msgid ""
"<A HREF=\"h_config_auto_include_reply\">&quot;Include-Text-In-Reply&quot;</"
"A>,"
msgstr ""

#: pith/helptext.c:6847
msgid ""
"<A HREF=\"h_config_attach_in_reply\">&quot;Include-Attachments-In-Reply&quot;"
"</A>,"
msgstr ""

