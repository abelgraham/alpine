#: pith/helptext.c:24700 pith/helptext.c:24785
msgid " <LI> <A HREF=\"h_index_sort_ordsubj\">OrderedSubj</A>"
msgstr ""

#: pith/helptext.c:24701 pith/helptext.c:24786
msgid " <LI> <A HREF=\"h_index_sort_thread\">Thread</A>"
msgstr ""

#: pith/helptext.c:24702 pith/helptext.c:24787
msgid " <LI> <A HREF=\"h_index_sort_from\">From</A>"
msgstr ""

#: pith/helptext.c:24703 pith/helptext.c:24788
msgid " <LI> <A HREF=\"h_index_sort_size\">Size</A>"
msgstr ""

#: pith/helptext.c:24704 pith/helptext.c:24789
msgid " <LI> <A HREF=\"h_index_sort_score\">Score</A>"
msgstr ""

#: pith/helptext.c:24705 pith/helptext.c:24790
msgid " <LI> <A HREF=\"h_index_sort_to\">To</A>"
msgstr ""

#: pith/helptext.c:24706 pith/helptext.c:24791
msgid " <LI> <A HREF=\"h_index_sort_cc\">Cc</A>"
msgstr ""

#: pith/helptext.c:24711 pith/helptext.c:24796
msgid "Each type of sort may also be reversed."
msgstr ""

#: pith/helptext.c:24712 pith/helptext.c:24797
msgid "Normal default is by &quot;Arrival&quot;."
msgstr ""

#: pith/helptext.c:24717
msgid "of the Sort-Key."
msgstr ""

#. TRANSLATORS: Start of new help topic.
#: pith/helptext.c:24741
msgid "<TITLE>OPTION: Set Startup Rule</TITLE>"
msgstr ""

#: pith/helptext.c:24744
msgid "<H1>OPTION: Set Startup Rule</H1>"
msgstr ""

#: pith/helptext.c:24747
