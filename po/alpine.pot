#: pith/helptext.c:24807
msgid ""
"(<!--chtml if pinemode=\"function_key\"-->F7<!--chtml else-->$<!--chtml "
"endif-->)"
msgstr ""

#: pith/helptext.c:5500 pith/helptext.c:9780 pith/helptext.c:18083
#: pith/helptext.c:32353
msgid "command."
msgstr ""

#: pith/helptext.c:5501
msgid "Your sorting options are:"
msgstr ""

#: pith/helptext.c:5504
msgid " <LI> <A HREF=\"h_index_sort_arrival\">A</A>rrival"
msgstr ""

#: pith/helptext.c:5505
msgid " <LI> <A HREF=\"h_index_sort_date\">D</A>ate"
msgstr ""

#: pith/helptext.c:5506
msgid " <LI> <A HREF=\"h_index_sort_subj\">S</A>ubject"
msgstr ""

#: pith/helptext.c:5507
msgid " <LI> <A HREF=\"h_index_sort_ordsubj\">O</A>rderedSubject"
msgstr ""

#: pith/helptext.c:5508
msgid " <LI> t<A HREF=\"h_index_sort_thread\">H</A>read"
msgstr ""

#: pith/helptext.c:5509
msgid " <LI> <A HREF=\"h_index_sort_from\">F</A>rom"
msgstr ""

#: pith/helptext.c:5510
msgid " <LI> si<A HREF=\"h_index_sort_size\">Z</A>e"
msgstr ""

#: pith/helptext.c:5511
msgid " <LI> scor<A HREF=\"h_index_sort_score\">E</A>,"
msgstr ""

#: pith/helptext.c:5512
msgid " <LI> <A HREF=\"h_index_sort_to\">T</A>o"
msgstr ""

#: pith/helptext.c:5513
msgid " <LI> <A HREF=\"h_index_sort_cc\">C</A>c"
msgstr ""

#: pith/helptext.c:5518
msgid "The Reverse option will toggle the order the index is currently"
